@model IEnumerable<WebAppMVC.Models.ViewModel.Employees.VWEmployeeList>

<div>
    <h4>Employee</h4>
    <hr />
    @foreach(var item in Model)
    {
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayFor(x => item.Title)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(x=>item.EmployeeId)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayFor(x=>item.HireDate)
            </dt>
        </dl>
    }
    
</div>
